import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
    Scanner scan=new Scanner(System.in);
        int n=scan.nextInt();
        int a=0;
        
        while(n>0){
            int b= n % 10;
            a=a * 10 + b;
            n = n /10;
        }
    System.out.println(a);
    }
}
