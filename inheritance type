class Parent {
    void display() {
        System.out.println("Parent");
    }
}

class Child extends Parent {
    void show() {
        System.out.println("Child");
    }
}

public class Main {
    public static void main(String[] args) {
        Parent p = new Parent();
        p.display();
        
        Child c = new Child();
        c.show();
    }
}
